{
  "breakpoints": {
    "values": {
      "xs": 0,
      "sm": 600,
      "md": 1094,
      "lg": 1280,
      "xl": 1920
    }
  },
  "props": {
    "MuiButtonBase": {
      "disableRipple": true
    }
  },
  "palette": {
    "common": {
      "black": "#050505",
      "white": "#fff",
      "icon": "#66666b"
    },
    "background": {
      "paper": "#fff",
      "default": "#fafafa"
    },
    "primary": {
      "light": "##CED6F2",
      "main": "#899ce1",
      "dark": "#2a44a2",
      "contrastText": "#fff"
    },
    "secondary": {
      "main": "#705880",
      "contrastText": "#fff"
    },
    "error": {
      "light": "#e57373",
      "main": "#f44336",
      "dark": "#d32f2f",
      "contrastText": "#fff"
    },
    "text": {
      "primary": "#202124",
      "secondary": "rgba(0, 0, 0, 0.54)",
      "disabled": "rgba(0, 0, 0, 0.38)",
      "hint": "rgba(0, 0, 0, 0.38)"
    }
  },
  "overrides": {
    "MuiAppBar": {
      "root": {
        "box-shadow": "none",
        "color": "#fff",
        "border-bottom": "0px",
        "background-color": "#fff",
        "z-index": 1201
      }
    },
    "MuiDataGridPanel": {
      "root": {
        "z-index": 1
      }
    },
    "MuiToggleButton": {
      "root": {
        "color": "#5F6368",
        "text-transform": "none",
        "font-weight": 400,
        "padding": "5px",
        "height": "36px",
        "border-color": "#E0E0E0",
        "&:hover, &.Mui-selected, &.MuiButtonBase-root.Mui-selected:hover": {
          "background-color": "#8DDDC6",
          "color": "#1CBC8D",
          "border-color": "#8DDDC6"
        },
        "&.Mui-selected": {
          "font-weight": 500,
          "color": "#1CBC8D"
        }
      }
    },
    "MuiButtonGroup": {
      "groupedOutlinedPrimary": {
        "&.selected, &:hover": {
          "color": "#1CBC8D",
          "background-color": "#8DDDC6",
          "border-color": "#8DDDC6"
        },
        "border": "1px solid rgba(0, 0, 0, 0.14)",
        "color": "#333333",
        "text-transform": "none",
        "font-weight": "normal",
        "height": "45px",
        "min-width": "98px"
      }
    },
    "MuiTableCell": {
      "stickyHeader": {
        "background-color": "#F5F5F5",
        "padding": "12px 25px",
        "font-weight": 500,
        "border-bottom": "1px solid #E0E0E0",
        "border-top": "1px solid #E0E0E0",
        "&:not(.spacer):not(.noborder):not(:last-child):after": {
          "content": "''",
          "height": "23px",
          "width": "0px",
          "right": "0px",
          "top": "10px",
          "border-right": "1px solid #E0E0E0"
        }
      }
    },
    "MuiTypography": {
      "h5": {
        "font-weight": 500,
        "font-size": "1.25rem",
        "color": "#333333"
      },
      "subtitle1": {
        "color": "#333333",
        "font-weight": 400,
        "font-size": "1rem"
      },
      "subtitle2": {
        "color": "#70757A",
        "font-weight": 400,
        "font-size": "0.875rem"
      }
    },
    "MuiExpansionPanel": {
      "root": {
        "box-shadow": "0px 1px 4px rgba(0, 0, 0, 0.12)",
        "border-radius": "4px"
      }
    },
    "MuiExpansionPanelSummary": {
      "content": {
        "margin": "4px 0px",
        "&.Mui-expanded": {
          "margin": "4px 0px"
        }
      }
    },
    "MuiLinearProgress": {
      "colorPrimary": {
        "background-color": "#E5E5E9",
        "height": "8px",
        "border-radius": "4px"
      },
      "barColorPrimary": {
        "background-color": "#050505"
      }
    },
    "MuiCheckbox": {
      "root": {
        "&.Mui-selected": "rgba(28, 188, 141, 1)",
        "color": "#DADADA",
        "margin-right": "15px",
        "margin-left": "5px",
        "height": "30px",
        "width": "30px"
      }
    },
    "MuiTabs": {
      "root": {
        "border-bottom": "1px solid #e8e8e8",
        "margin-bottom": "10px"
      }
    },
    "MuiTab": {
      "root": {
        "text-transform": "capitalize",
        "font-weight": "500",
        "font-style": "normal",
        "font-size": "14px",
        "line-height": "16.41px",
        "letter-spacing": "0.1px",
        "font-family": "\"Roboto Slab\", serif"
      }
    },
    "MuiCard": {
      "root": {
        "&:hover": {
          "cursor": "pointer",
          "box-shadow": "rgba(0, 0, 0, 0.2) 0px 5px 5px 0px, rgba(0, 0, 0, 0.14) 0px 8px 10px 0px, rgba(0, 0, 0, 0.12) 0px 3px 14px 0px"
        }
      }
    }
  }
}